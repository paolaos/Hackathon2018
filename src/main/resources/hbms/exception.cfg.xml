<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="application.model.Exception" table="EXCEPTION" catalog="CODE_MERGERS">
        <id name="exceptionId">
            <column name="exception_id" sql-type="int" not-null="true"/>
        </id>
        <property name="indicatorFk" insert="false" update="false">
            <column name="indicator_fk" sql-type="int"/>
        </property>
        <property name="workProcessFk">
            <column name="work_process_fk" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="processMeasureFk">
            <column name="process_measure_fk" sql-type="varchar(255)" length="255" not-null="true"/>
        </property>
        <property name="dateCreated">
            <column name="date_created" sql-type="date" not-null="true"/>
        </property>
        <property name="priority">
            <column name="priority" sql-type="varchar(10)"/>
        </property>
        <property name="tolerance">
            <column name="tolerance" sql-type="double" not-null="true"/>
        </property>
        <property name="value_kpi">
            <column name="value_kpi" sql-type="double" not-null="true"/>
        </property>
        <property name="status">
            <column name="status" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="description">
            <column name="description" sql-type="varchar(255)" not-null="false"/>
        </property>
        <many-to-one name="processMeasure" class="application.model.ProcessMeasure" insert="false" update="false">
            <column name="indicator_fk"/>
            <column name="work_process_fk"/>
            <column name="process_measure_fk"/>
        </many-to-one>
    </class>

</hibernate-mapping>