<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="application.model.Indicators" table="INDICATORS" catalog="CODE_MERGERS">
        <id name="id">
            <column name="id" sql-type="int" length="60"/>
            <generator class="increment"/>
        </id>
        <property name="name">
            <column name="name" sql-type="varchar(60)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>