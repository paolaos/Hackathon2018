<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="application.model.WorkProcess" table="WORK_PROCESS" catalog="CODE_MERGERS">
        <composite-id name="workProcessId" class="application.model.WorkProcessId">
            <key-property name="indicatorId" column="indicator_id"></key-property>
            <key-property name="name" column="name"></key-property>
        </composite-id>
        <many-to-one name="indicator" class="application.model.Indicators" column="indicator_id" lazy="false"/>
    </class>

</hibernate-mapping>